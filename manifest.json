{
    "name": "beautiExtension",
    "version": "1.0",
    "description": "replaces all images",
    "manifest_version": 2,
    
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["script.js"]
    }],
  "browser_action": {
        "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
    },
    "icons": {
       "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    }

}
